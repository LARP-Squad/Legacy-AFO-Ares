	<!---------------------------------------------------Concealed IIIA armor ------------------------------------------------->
    <Bind eqp="Delta_Jacket_Tan_Concealed1">
        <to name="isa1"/>

        </Bind>
    <Armor name="Delta_Jacket_Tan_Concealed1" inventoryBinding="Armor" unlockCost="0"
            tooltip="Jacket Tan + Concealed"
            description="Concealable Level IIIA soft body armor worn with a jacket. Allows for covert operations, and offers some protection from handgun projectiles."
            img="data/models/equipment/armor_softpanelsiiia_ui.dds"
            img_tooltip="data/models/equipment/armor_01_ui_ttip.tga"
        >
	<RenderObject3D model="data/models/humans/swat_militia.khm" diffuseTex="data/models/humans/isa1.dds"/>
        <RenderObject3D model="data/models/humans/attachments/cia_jacket.khm" diffuseTex="data/models/humans/attachments/cia_jacket.dds" windAnimated="1"/>
        <MobilityModifiers moveSpeedModifierPercent="-5" turnSpeedModifierPercent="-3"/>
        <ConcealmentModifier add="9"/>

        <Params protectionInfoText="@equipment_protect1">
            <ProtectionArc degrees="90" coveragePercent="30" piercingProtectionLevel="3"/>
            <ProtectionArc degrees="270" coveragePercent="10" piercingProtectionLevel="1"/>
            <ProtectionArc degrees="360" coveragePercent="30" piercingProtectionLevel="3"/>
        </Params>
    </Armor>
